Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{"Clear", "[", 
  RowBox[{
  "a0max", ",", "a0t", ",", "wmin", ",", "\[Theta]", ",", "wavg", ",", "fs", 
   ",", "\[Psi]", ",", "\[Theta]2nd", ",", "\[Gamma]avg", ",", "\[Beta]", ",",
    "fp"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Clear", "[", 
   RowBox[{
   "\[Tau]", ",", "W0", ",", "\[Lambda]", ",", "c", ",", "\[Omega]0", ",", 
    "a0", ",", "k0", ",", "np", ",", "nc"}], "]"}], "\[IndentingNewLine]", 
  "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{"equation", " ", "1"}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"a0max", "=", 
    FractionBox[
     RowBox[{"np", " ", "\[Pi]", " ", "l"}], 
     RowBox[{"nc", " ", "\[Lambda]"}]]}], ";"}], "\[IndentingNewLine]", 
  "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{"equation", " ", "3"}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"a0t", "=", 
    FractionBox[
     RowBox[{
      RowBox[{"4", "/", "3"}], " ", 
      RowBox[{"\[Sqrt]", "2"}], " ", 
      RowBox[{"k0", "^", "2"}], " ", 
      RowBox[{"l", "^", "2"}], " ", "np"}], 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"3", "/", "4"}], "+", 
        RowBox[{
         RowBox[{"8", "/", "3"}], 
         RowBox[{"\[Sqrt]", "2"}], " ", "k0", " ", "l"}]}], ")"}], "nc"}]]}], 
   ";"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{"equation", " ", "6"}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"wmin", "=", 
    RowBox[{"W0", " ", 
     RowBox[{"Sqrt", "[", 
      RowBox[{"Abs", "[", 
       RowBox[{"Log", "[", 
        RowBox[{"a0max", "/", "a0"}], "]"}], "]"}], "]"}]}]}], ";"}], 
  "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{"equation", " ", "7"}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"\[Theta]", "=", 
    RowBox[{"ArcTan", "[", 
     RowBox[{
      FractionBox[
       RowBox[{"\[Tau]", " ", "c"}], 
       RowBox[{
        RowBox[{"Sqrt", "[", 
